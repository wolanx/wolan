type Query {
    # 通过id获取
    getTaskBySid(name: String!): TaskDO
    # 信息列表
    listTask: [TaskDO]
}

# 信息对象
type TaskDO {
    sid: String!
    name: String!
    version : String
    git: GitBean
}

type GitBean {
    url:String
    branch:String
}

type Mutation {
    createTask(TaskDO: addTask!): TaskDO
}

input addTask {
    sid: String!
    name: String!
}

