# get

type Query {
    # 通过id获取
    getTaskBySid(name: String!): TaskDO
    # 信息列表
    listTask: [TaskDO]
}

# post

type Mutation {
    createTask(TaskDO: createTaskInput!): TaskDO
    deleteTask(sid: String!): Boolean
}

# obj

type TaskDO {
    sid: String!
    name: String!
    version : String
    git: GitBean
}

type GitBean {
    url:String
    branch:String
}

# input

input createTaskInput {
    name: String!
    git : gitInput!
}

input gitInput{
    url: String!
    branch : String!
}

